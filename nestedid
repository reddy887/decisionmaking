class nestedif
{
  public static void main (String args[])
   {
      int num=23;
      if (num>20)
       {
          if (num<20)
	   {  
	      System.out.println(" the number is more then 20");
	    }
	   else
	    {
	       System.out.println(num);
	     }
	  System.out.println("the number is above 20");
	}
      else
      {
        System.out.println(num);
	}
    }
}
